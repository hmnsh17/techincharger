<!-- contact section -->
<section class="contact_section layout_padding">
  <div class="container">
    <div class="heading_container heading_center">
      <h2>Get In <span>Touch</span></h2>
      <% if flash[:notice].present? %>
        <div class="alert alert-success">
          <%= flash[:notice] %>
        </div>
      <% elsif flash[:alert].present? %>
        <div class="alert alert-danger">
          <%= flash[:alert] %>
        </div>
      <% end %>
    </div>
    <div class="row">
      <div class="col-md-6 px-0">
        <div class="form_container">
          <%= form_with(model: @contact, url: contact_path, local: true) do |form| %>
            <div class="form-row">
              <div class="form-group col">
                <%= form.text_field :name, class: "form-control", placeholder: "Your Name" %>
              </div>
            </div>
            <div class="form-row">
              <div class="form-group col-lg-6">
                <%= form.text_field :phone, class: "form-control", placeholder: "Phone Number" %>
              </div>
              <div class="form-group col-lg-6">
                <%= form.select :service, options_for_select(["Select Service", "Home Welding", "Gate Welding", "Window Welding", "Machine Welding", "Bike Welding", "Car Welding"]), {}, { class: "form-control wide" } %>
              </div>
            </div>
            <div class="form-row">
              <div class="form-group col">
                <%= form.email_field :email, class: "form-control", placeholder: "Email" %>
              </div>
            </div>
            <div class="form-row">
              <div class="form-group col">
                <%= form.text_area :message, class: "message-box form-control", placeholder: "Message" %>
              </div>
            </div>
            <div class="btn_box">
              <%= form.submit "SEND", class: "btn btn-primary" %>
            </div>
          <% end %>
        </div>
      </div>
      <div class="col-md-6 px-0">
        <div class="map_container">
          <div class="map">
            <div id="googleMap"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
<!-- end contact section -->

